<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <Button
        android:id="@+id/btn_mall2"
        android:layout_width="260dp"
        android:layout_height="90dp"
        android:layout_marginEnd="108dp"
        android:layout_marginRight="108dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_style_ordinary"
        android:text="试剂商城"
        android:textColor="@android:color/background_light"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/record_query" />

    <ImageView
        android:id="@+id/U_bg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/reagent_operation"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="100dp"
        android:background="@drawable/u_bg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/orinary_template"
        android:layout_width="260dp"
        android:layout_height="90dp"
        android:layout_alignLeft="@+id/edit_flie"
        android:layout_alignStart="@+id/edit_flie"
        android:layout_marginStart="108dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_style_ordinary"
        android:text="试剂模板"
        android:textColor="@android:color/background_light"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edit_flie" />

    <Button
        android:id="@+id/reagent_operation"
        android:layout_width="580dp"
        android:layout_height="100dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="100dp"
        android:background="@drawable/btn_style_kuaisuquhuan"
        android:text="试剂操作"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/regentsearch"
        android:layout_width="580dp"
        android:layout_height="100dp"
        android:layout_alignLeft="@+id/reagent_operation"
        android:layout_alignStart="@+id/reagent_operation"
        android:layout_below="@+id/reagent_operation"
        android:layout_marginTop="10dp"
        android:background="@drawable/btn_style_kuaisuquhuan"
        android:text="试剂查找"
        android:textColor="@android:color/background_light"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/reagent_operation" />

    <Button
        android:id="@+id/edit_flie"
        android:layout_width="260dp"
        android:layout_height="90dp"
        android:layout_alignEnd="@+id/regentsearch"
        android:layout_alignRight="@+id/regentsearch"
        android:layout_marginEnd="432dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_style_ordinary"
        android:text="账号信息"
        android:textColor="@android:color/background_light"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/regentsearch" />

    <Button
        android:id="@+id/record_query"
        android:layout_width="260dp"
        android:layout_height="90dp"
        android:layout_marginEnd="108dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_style_ordinary"
        android:text="记录查询"
        android:textColor="@android:color/background_light"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/edit_flie"
        app:layout_constraintTop_toBottomOf="@+id/regentsearch" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:background="@drawable/jiluchaxun_icon"
        app:layout_constraintBottom_toBottomOf="@+id/orinary_template"
        app:layout_constraintEnd_toEndOf="@+id/orinary_template"
        app:layout_constraintHorizontal_bias="0.12"
        app:layout_constraintStart_toStartOf="@+id/orinary_template"
        app:layout_constraintTop_toTopOf="@+id/orinary_template" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:background="@drawable/shijichazhao_icon"
        app:layout_constraintBottom_toBottomOf="@+id/edit_flie"
        app:layout_constraintEnd_toEndOf="@+id/edit_flie"
        app:layout_constraintHorizontal_bias="0.12"
        app:layout_constraintStart_toStartOf="@+id/edit_flie"
        app:layout_constraintTop_toTopOf="@+id/edit_flie" />

    <ImageView
        android:id="@+id/search_icon"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:background="@drawable/jiluchaxun_icon"
        app:layout_constraintBottom_toBottomOf="@+id/record_query"
        app:layout_constraintEnd_toEndOf="@+id/record_query"
        app:layout_constraintHorizontal_bias="0.12"
        app:layout_constraintStart_toStartOf="@+id/record_query"
        app:layout_constraintTop_toTopOf="@+id/record_query" />


</android.support.constraint.ConstraintLayout>
